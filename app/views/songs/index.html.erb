<h1><%= "#{@artist.name}'s Songs" %></h1>
<hr />
  <%= link_to 'Add a Song', new_artist_song_path(@artist) %>
<ul>
  <% @song.each do |song| %>
    <li><%= link_to song.title, [@artist, song] %><%="  Album: #{song.album}  Time: #{song.time}"%></li>
    <%= link_to 'Edit', edit_artist_song_path(@artist, song) %>
    <%= link_to 'Delete', [@artist, song], method: :delete %>
    <% end %>

    <br />
    <br />
    <hr />
    <%= link_to 'Back to Artists', artists_path %>
    <%= link_to 'Back to Billboards', bboards_path %>
    
  
</ul>
